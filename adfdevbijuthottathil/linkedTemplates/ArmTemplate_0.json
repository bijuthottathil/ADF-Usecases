		"lnkPostgreSqlAzure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"lnkPostgreSqlAzure_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "bijupostgres"
		}
